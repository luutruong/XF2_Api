{
    "require": {
        "php": ">=7.4",
        "kreait/firebase-php": "6.9.6",
        "ext-openssl": "*",
        "ext-json": "*",
        "google/apiclient": "2.12.6"
    },
    "config": {
        "autoloader-suffix": "TruonglvApi"
    },
    "prefer-stable": true,
    "replace": {
        "guzzlehttp/guzzle": "*",
        "guzzlehttp/promises": "*",
        "guzzlehttp/psr7": "*",
        "psr/log": "*",
        "psr/http-message": "*",
        "symfony/polyfill-mbstring": "*",
        "paragonie/random_compat": "*",
        "paragonie/constant_time_encoding": "*"
    },
    "scripts": {
        "post-autoload-dump": ["Google\\Task\\Composer::cleanup"]
    },
    "extra": {
        "google/apiclient-services": [
            "AndroidPublisher"
        ]
    }
}
