{
    "require": {
        "php": ">=8.0.0",
        "ext-openssl": "*",
        "ext-json": "*",
        "google/apiclient": "^2.18"
    },
    "config": {
        "autoloader-suffix": "TruonglvApi"
    },
    "prefer-stable": true,
    "replace": {
        "guzzlehttp/guzzle": "*",
        "guzzlehttp/promises": "*",
        "guzzlehttp/psr7": "*",
        "psr/log": "*",
        "psr/cache": "*",
        "psr/http-message": "*",
        "symfony/polyfill-mbstring": "*",
        "paragonie/random_compat": "*",
        "paragonie/constant_time_encoding": "*",
        "brick/math": "*",
        "symfory/polyfill-php80": "*"
    },
    "scripts": {
        "post-autoload-dump": ["Google\\Task\\Composer::cleanup"]
    },
    "extra": {
        "google/apiclient-services": [
            "AndroidPublisher"
        ]
    }
}
